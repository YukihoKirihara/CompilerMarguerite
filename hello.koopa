global @A = alloc i32, zeroinit

decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @thisisatestfunction() {
%entry:
  jump %0

%0:
  jump %exit

%exit:
  ret
}

fun @main(): i32 {
%entry:
  %ret = alloc i32
  jump %1

%1:
  store 1, @A
  store 0, %ret
  jump %exit

%2:
  jump %exit

%exit:
  %3 = load %ret
  ret %3
}
