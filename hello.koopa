global @y = alloc i32, zeroinit

decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @main(): i32 {
%entry:
  %ret = alloc i32
  @x = alloc i32
  jump %0

%0:
  %1 = mul 2, 3
  %2 = add 1, %1
  store %2, @x
  %3 = load @x
  store %3, %ret
  jump %exit

%4:
  jump %exit

%exit:
  %5 = load %ret
  ret %5
}
