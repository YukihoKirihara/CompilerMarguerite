fun @main(): i32 {
%entry:
  %ret = alloc i32
  jump %0

%0:
  br 0, %if_true, %if_false

%if_true:
  store 1, %ret
  jump %exit

%1:
  jump %if_final

%if_false:
  store 2, %ret
  jump %exit

%2:
  jump %if_final

%if_final:
  store 3, %ret
  jump %exit

%3:
  jump %exit

%exit:
  %4 = load %ret
  ret %4
}
