fun @main(): i32 {
%entry:
  %ret = alloc i32
  jump %0

%0:
  @i = alloc i32
  store 0, @i
  jump %while_cond

%while_cond:
  %1 = load @i
  %2 = lt %1, 10
  br %2, %while_body, %while_final

%while_body:
  %3 = load @i
  %4 = add %3, 1
  store %4, @i
  jump %while_cond

%while_final:
  %5 = load @i
  store %5, %ret
  jump %exit

%6:
  jump %exit

%exit:
  %7 = load %ret
  ret %7
}
